@startuml' =========================' Base Value Objects & Enums' =========================enum VacationType {  OPEN  REQUESTED  SYSTEM_SUGGESTION  CONFIRMED}' =========================' Core Domain' =========================class AnnualPlan {    - year: Int    - generateAnnualPlan(employeeManagement.Employees)}object calendarCellResolver {    +cellText(employee: Employee, year: Int, week: UInt)}interface CalendarOutput {  + printYearPlan(year: Int, employees: List<Employee>)}class ConsoleCalendarOutput {    -val weeksPerYear: Int = 52,    -val cellWidth: Int = 6,    -val minEmpColWidth: Int = 4    +printYearPlan(year: Int, employees: List<Employee>)    -buildHeader(empColWidth: Int): String = buildString    -buildRow(employee: Employee, year: Int, empColWidth: Int): String = buildString    -padRight(text: String, width: Int): String    -trimToWidth(text: String, width: Int): String}class Employee {  - val id: UInt  - var firstName: String  - var lastName: String  - var workloadPercent: UByte  - var role: Role = Role.STAFF  - val qualifications: MutableList<Qualification> = mutableListOf()  - val vacationEntries: MutableList<VacationEntry>  Employee(id)  + getId()  + getFullName()  + getWorkloadPercent()  + getRole()  + getQualifications()  + getVacationEntries()  + getAbbreviation()  + setFirstName()  + setLastName()  + setWorkloadPercent()  + setRole()  + addQualification()  + removeQualification()  + addVacationEntry()  + clearVacationEntries()}object employeeManagement {     - val employees: MutableList<Employee>     + seedEmployees()     + add()     + removeById()     + findById()     + findByAbbreviation()     + listAll()}class IdProvider{    var start: UInt    var nextId: UInt = start    var id : UInt    val issuedIds : mutableSetOf<UInt>    --    IdProvider(start)    generateId(): UInt    Issued() : Boolean}enum MessageType {    INFO,    WARNING,    ERROR}class Qualification{val name: StringQualification(name)}enum Role {    APPRENTICE,    STAFF,    TEAM_LEAD,    MANAGER}interface UserInterface {     + showMessage(msg: String, type: MessageType = MessageType.INFO)     + readLine(prompt: String = ""): String?}class VacationEntry {  + val id: UInt  + val employeeId: UInt  + val year: Int  - val weekStatus: MutableMap<UInt, VacationStatus>  + weeks(): Set<UInt>  + hasWeek(week: UInt): Boolean  + getStatus(week: UInt): VacationStatus?  + setStatus(week: UInt, status: VacationStatus)  + addWeek(week: UInt, status: VacationStatus = VacationStatus.GENERATED)  + addRange(range: WeekRange, status: VacationStatus = VacationStatus.GENERATED)}object VacationPlanner {    + generateForAll(year: Int, employees: List<Employee>, nextVacationId: () -> UInt)    - addTwoWeekBlock(entry: VacationEntry, startWeek: UInt)}enum VacationStatus {    GENERATED,    APPROVED,    REJECTED}class WeekRange <<Data Class>> {  + val startWeek: UInt  + val endWeek: UInt  WeekRange( startWeek, endWeek)  + covers(week: UInt): Boolean}class GUI {}' =========================' Relationships' =========================AnnualPlan --> employeeManagementAnnualPlan <|-- UserInterfaceemployeeManagement --> "1..n" EmployeeEmployee --> "1..n" VacationEntryEmployee --> QualificationEmployee --> RoleVacationEntry --> "1..n     " WeekRangeIdProvider <-- employeeManagementIdProvider <-- VacationEntryIdProvider <-- EmployeeGUI <-- UserInterfaceUserInterface --> MessageTypeVacationEntry --> VacationTypeVacationEntry --> VacationStatusVacationPlanner --> VacationStatusConsoleCalendarOutput --> calendarCellResolverConsoleCalendarOutput <|-- CalendarOutputnote top of AnnualPlan  Annual plan for an organization.  Employee-centered: all vacations  are stored per employee.end notenote right of VacationEntry  Vacation block for an employee.  Based on week ranges.  Can be extended to day ranges.end note@enduml