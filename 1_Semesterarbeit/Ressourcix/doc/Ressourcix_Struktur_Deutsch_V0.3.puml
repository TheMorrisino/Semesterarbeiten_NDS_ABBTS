@startuml' =========================' Basis-Werteobjekte & Enums' =========================enum FerienArt {  OFFEN  WUNSCH  SYSTEM_VORSCHLAG  BESTAETIGT}' =========================' Kern-Domäne' =========================class JahresPlan {    - jahr: Int    - generiertJahresplan(mitarbeiterVerwaltung.Miterarbeiters)}object mitarbeiterVerwaltung{     - Miterarbeiters: Mutiblelist <Mitarbeiter>     + einfuege()     + lösche()     + sucheNachId()     + sucheNachKuerzel()}class Mitarbeiter {  - id: Uint  - vorname: String  - nachname: String  - ferienEintraege: Mutiblelist<FerienEintrag>  Mitarbeiter(id, vorname, nachname, pensumProzent)  + change()  + generiereFerienantrag()  + generiereKürzel()}class FerienEintrag {  - id: Uint  - holidayWeek = HolidayWeek  - holidayWeeks : MutableList<HolidayWeek>  - art: FerienArt  + addHolidayWeek()  + removeHolidayWeek()  + changeHolidayWeek()  + toString()}  class  WochenBereich <<Data Class>> {  - startWoche: Uint  - endWoche: Uint}class idManager {    var ids : List <Int>    CheckId() : Int    GeneriereID() : Int}class GUI {}interface Benutzerschnittstelle {    Kalender: List<Mitarbeiters>}interface Datenbank {}interface Datenbank {}' =========================' Beziehungen' =========================JahresPlan -->  mitarbeiterVerwaltungJahresPlan <|-- BenutzerschnittstellemitarbeiterVerwaltung --> "1..n    " MitarbeiterMitarbeiter --> "1..n    "  FerienEintragFerienEintrag  --> "1..n  "  WochenBereichidManager  <--   mitarbeiterVerwaltungidManager  <--   FerienEintragidManager  <--   MitarbeiterGUI <-- BenutzerschnittstelleFerienEintrag --> FerienArtnote top of JahresPlan  Jahresplan für eine Organisation.  Mitarbeiterzentriert: Alle Ferien werden pro Mitarbeiter gespeichert.end notenote right of FerienEintrag  Ferienblock für einen Mitarbeiter.  Basierend auf Wochenbereichen.  Erweiterbar auf Tagesbereiche.end note@enduml